{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "cordova",
    ],
    "paths": { "@angular/*": ["node_modules/@angular/*"] },
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "node_modules/jqwidgets-scripts/jqwidgets-ts/**/*.ts"
  ]
}
